apiVersion: apps/v1
kind: Deployment
metadata:
  name: client-deployment
  namespace: qa
spec:
  replicas: 1
  selector:
    matchLabels:
      app: client
  template:
    metadata:
      labels:
        app: client
    spec:
      containers:
        - name: client
          image: ${{ secrets.ECR_CLIENT_URI }}:latest
          ports:
            - containerPort: 80
